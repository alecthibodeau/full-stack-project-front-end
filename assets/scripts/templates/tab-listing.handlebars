{{#each tabs as |tab|}}
  {{log tab}}
  <!-- {{> partial}} -->

  <tr data-id="{{tab.id}}"
    data-date="{{tab.date}}"
    data-project_name="{{tab.project_name}}"
    data-task="{{tab.task}}"
    data-time_spent="{{tab.time_spent}}"
    data-notes="{{tab.notes}}"
    >
    <td class="info-td">
      {{#if tab.date}}
        {{tab.date}}
      {{else}}
        —
      {{/if}}
    </td>
    <td class="info-td">
      {{#if tab.project_name}}
        {{tab.project_name}}
      {{else}}
        —
      {{/if}}
    </td>
    <td class="info-td">
      {{#if tab.task}}
        {{tab.task}}
      {{else}}
        —
      {{/if}}
    </td>
    <td class="info-td">
      {{#if tab.time_spent}}
        {{tab.time_spent}}
      {{else}}
        —
      {{/if}}
    </td>
    <td class="info-td">
      {{#if tab.notes}}
        {{tab.notes}}
      {{else}}
        —
      {{/if}}
    </td>
    <td class="info-button"><button type="button" class="update-button" id="update-button" data-toggle="modal" data-target="#updateTabModal">Update</td>
    <td class="info-button"><button type="submit" class="delete-button" id="delete-button">Delete</button></td>
  </tr>

{{/each}}
